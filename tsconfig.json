{
  "compilerOptions": {
    "lib": [
        "ES2017",
        "webworker"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist",
    "skipLibCheck": true,
    "types": [
        "node",
    ],
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
},
"include": [
    "src"
]
}
